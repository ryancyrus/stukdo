<paper-scroll-header-panel>
<!-- Drawer Toolbar -->
<paper-toolbar>
	<paper-tabs id='tabs' selected='home' self-end>
		<% if current_user %>
		<paper-tab name='home'><%= link_to 'Tasks' %></paper-tab>
		<paper-tab name='home'><%= link_to "Settings", edit_user_registration_path %></paper-tab>
		<paper-tab name='logout'><%= link_to "Logout", destroy_user_session_path, method: :delete %></paper-tab>
		<% else %>
		<paper-tab name='signup'><%= link_to "Signup".html_safe, new_user_registration_path %></paper-tab>
		<paper-tab name='login'><%= link_to "Login".html_safe, new_user_session_path %></paper-tab>
		<% end %>
	</paper-tabs>
</paper-toolbar>
</paper-scroll-header-panel>


